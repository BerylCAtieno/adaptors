{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "apiVersion": {
      "title": "API Version",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "placeholder": "v3",
      "default": "v3",
      "description": "PDFShift api version",
      "minLength": 1,
      "examples": [
        "v3"
      ]
    },
    "apiKey": {
      "title": "API Key",
      "type": "string",
      "description": "Your PDFShift API key",
      "writeOnly": true,
      "examples": ["your-pdfshift-api-key"]
    }
  },
  "type": "object",
  "additionalProperties": true,
  "required": ["apiKey"]
}
